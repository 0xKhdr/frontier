{
	"name": "frontier/frontier",
	"description": "Laravel Frontier Starter Kit",
	"type": "library",
	"version": "1.0",
	"license": "MIT",
	"require": {
		"php": ">=8.2",
		"laravel/framework": "^11.0|^12.0"
	},
	"require-dev": {
		"laravel/pint": "^v1.22.0"
	},
	"autoload": {
		"psr-4": {
			"Frontier\\": "src/",
			"Frontier\\Tests\\": "tests/"
		}
	},
	"minimum-stability": "stable",
	"extra": {
		"laravel": {
			"providers": [
                "Frontier\\Providers\\ServiceProvider",
				"Frontier\\Providers\\StarterServiceProvider"
			]
		}
	}
}
